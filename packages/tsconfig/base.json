{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Default",
  "compilerOptions": {
    "lib": ["ES2019"],
    "module": "CommonJS",
    "target": "ES2019",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "downlevelIteration": true
  },
  "exclude": ["node_modules"]
}
